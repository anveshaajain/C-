using System;
class animal
{
    public void speak()
    {
        Console.WriteLine("Animal can speak through their own language");
    }
}
class dog:animal
{
    public void bark()
    {
        Console.WriteLine("Dog barks");
    }
}
class program
{
    static void Main()
    {
        dog d= new dog();   
        d.speak();
        d.bark();
    }
}
