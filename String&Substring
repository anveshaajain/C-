using System;

class Substrings
{
    static void Main()
    {
        Console.WriteLine("Enter a string: ");
        string str = Console.ReadLine();

        Console.WriteLine("All substrings are: ");
        for (int i = 0; i < str.Length; i++)
        {
            for (int j = 1; j <= str.Length - i; j++)
            {
                Console.WriteLine(str.Substring(i, j));
            }
        }
    }
}
